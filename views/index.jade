doctype html
html(ng-app="bibliolabs-viz" manifest="/cache.manifest" type="text/cache-manifest")
  head
    link(rel='stylesheet', href='/css/styles.css') 
    link(rel='stylesheet', href="/bower_components/bootstrap/dist/css/bootstrap.min.css" )
    script(src='/bower_components/angular/angular.min.js')
    script(src='/bower_components/angular-route/angular-route.min.js')
    script(src='/bower_components/angular-animate/angular-animate.min.js')
    script(src='/bower_components/angular-bootstrap/ui-bootstrap-tpls.min.js')
    //script(src="/js/angular-resource.min.js")
    script(src='/js/app.js')
    //script(src='/js/d3.v3.min.js')

    title Sistema de Bibliotecas de Medell&iacute;n - Iniciativas 

  body
    div.page_wrapper
      div.content_wrapper
        header
          div.content
            div.row
              div.col-md-10
                img(src='/images/sbpm.png' alt="Sistema de Bibliotecas P&uacute;blicas de Medell&iacute;n")
              div.col-md-2
                img(src='/images/bibliolabs.png' alt="Bibliolabs")

        
        div.title_bar
          h1.top INICIATIVAS
          h1.bottom BIBLIOTECAS
          
        div.main_content
          div(ng-view)


        footer
          div.content
            div.row
              div.col-md-12.col-sm-12.col-xs-12
                div#footer_container.even_layout
                  div.footer_image
                    img(src="/images/cc-sa4.png" alt="Creative Commons Share-Alike Version 4.0")
                  div.footer_image
                    img(src="/images/bibliolabs_logo.png" alt="Bibliolabs")
                  div.footer_image
                    img(src="/images/c3p.png" alt="CasaTresPatios")
                  div.footer_image
                    img(src="/images/proyecto_sbpm.png" alt="Sistema de Bibliotecas P&uacute;blicas de Medell&iacute;n")
                  div.footer_image
                    img(src="/images/lectura_viva.png" style="margin-top: 10px" alt="Lectura Viva")
                  div.footer_image
                    img(src="/images/todos_por_la_vida.png" alt="Medell&iacute;n Todos Por La Vida")
                  div.footer_image
                    img(src="/images/alcaldia_medellin.png" alt="Alcald&iacute;a de Medell&iacute;n")
